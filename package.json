{
  "name": "mjml-to-html-jsreport-recipe",
  "version": "0.0.0",
  "description": "jsreport studio extension to convert mjml to html",
  "keywords": [
    "jsreport",
    "mjml"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "test": "standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "prepublish": "jsreport-studio-build"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "MIT",
  "dependencies": {
    "mjml": "4.12.0"
  },
  "author": "Mohamed El-Refaie",
  "devDependencies": {
    "@jsreport/studio-dev": "3.0.1",
    "standard": "16.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
