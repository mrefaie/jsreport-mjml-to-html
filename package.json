{
  "name": "jsreport-mjml-to-html",
  "version": "0.0.1",
  "description": "jsreport studio recipe converting mjml to html",
  "keywords": [
    "jsreport",
    "mjml"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "test": "standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "prepublish": "jsreport-studio-build"
  },
  "homepage": "https://github.com/mrefaie/jsreport-mjml-to-html",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrefaie/jsreport-mjml-to-html.git"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "MIT",
  "dependencies": {
    "mjml": "4.12.0"
  },
  "author": {
    "name": "Mohamed El-Refaie",
    "email": "mohgamal@hayasolutions.com"
  },
  "devDependencies": {
    "@jsreport/studio-dev": "3.0.1",
    "standard": "16.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
